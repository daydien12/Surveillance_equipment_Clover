#Generating source browse information for project test


#Abbreviations
cc = C$:\Program$ Files$ (x86)\IAR$ Systems\Embedded$ Workbench$ 8.3\common\bin\SourceIndexer.exe
ll = C$:\Program$ Files$ (x86)\IAR$ Systems\Embedded$ Workbench$ 8.3\common\bin\PbdLink.exe
bd = C$:\Program$ Files$ (x86)\IAR$ Systems\Embedded$ Workbench$ 8.3\common\bin\makeBrowseData.exe



#Rules
rule index
  depfile = $out.dep
  command = $cc -out=$out -f $in
rule link
  command = $ll -M $out $in
rule browsedata
  command = $bd $in -output  $out



#Build steps
build stm8s_it.pbi : index stm8s_it.xcl
build dv_conver.pbi : index dv_conver.xcl
build dv_delay.pbi : index dv_delay.xcl
build dv_device_command.pbi : index dv_device_command.xcl
build dv_gpio.pbi : index dv_gpio.xcl
build dv_i2c.pbi : index dv_i2c.xcl
build dv_ringbuffer.pbi : index dv_ringbuffer.xcl
build dv_sht30.pbi : index dv_sht30.xcl
build dv_sys.pbi : index dv_sys.xcl
build dv_test.pbi : index dv_test.xcl
build dv_time.pbi : index dv_time.xcl
build dv_uart.pbi : index dv_uart.xcl
build main.pbi : index main.xcl
build test_part0.pbi : link stm8s_it.pbi dv_conver.pbi dv_delay.pbi dv_device_command.pbi
build test_part1.pbi : link dv_gpio.pbi dv_i2c.pbi dv_ringbuffer.pbi dv_sht30.pbi
build test_part2.pbi : link dv_sys.pbi dv_test.pbi dv_time.pbi dv_uart.pbi
build test_part3.pbi : link main.pbi
build test.pbd : link test_part0.pbi test_part1.pbi test_part2.pbi test_part3.pbi
build test.pbw : browsedata test.pbd


